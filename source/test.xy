int firstNum = stdInput("Enter First Number: ");
char Symbol = stdInput("Enter Action (+ - * /): ");
int SecondNum = stdInput("Enter Second Number: ");
int Result;

if (Symbol == '+')
{
    Result = firstNum + SecondNum;
}
elif (Symbol == '-')
{
    Result = firstNum - SecondNum;
}
elif (Symbol == '*')
{
    Result = firstNum * SecondNum;
}
elif (Symbol == '/')
{
    Result = firstNum / SecondNum;
}
else 
{
    stdOut("Incorrect Action Symbol\n");
    return 10;
}
stdOut("Result: ");
stdOut(Result);
stdOut("\n");

return 0;