int firstNum;
int secondNum;
int result;
char symbol;

firstNum = stdInput("Enter First Num: ");
symbol = stdInput("Enter Action Symbol: ");
secondNum = stdInput("Enter Second Num: ");

if (symbol == '+')
{
    result = firstNum + secondNum;
}
elif (symbol == '-')
{
    result = firstNum - secondNum;
}
elif (symbol == '*')
{
    result = firstNum * secondNum;
}
elif (symbol == '/')
{
    result = firstNum / secondNum;
}
else
{
    stdOut("You Entered Incorrect Symbol");
    return 1;
}
stdOut(result);
stdOut("\n");
return result;